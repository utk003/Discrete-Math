8c6d4bccac08"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"numPeople", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Dimensions", "[", "input", "]"}], "[", 
     RowBox[{"[", "2", "]"}], "]"}], "-", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"numItems", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"Dimensions", "[", "input", "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], "-", "1"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"people", "=", 
   RowBox[{"Drop", "[", 
    RowBox[{
     RowBox[{"input", "[", 
      RowBox[{"[", "1", "]"}], "]"}], ",", "1"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"items", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"input", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "2", ",", 
        RowBox[{"numItems", "+", "1"}]}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ratings", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"input", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "[", 
        RowBox[{"[", "j", "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "2", ",", 
         RowBox[{"numPeople", "+", "1"}]}], "}"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "2", ",", 
       RowBox[{"numItems", "+", "1"}]}], "}"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.8123720202191753`*^9, 3.812372094222937*^9}, {
  3.812372163576427*^9, 3.81237221378017*^9}, {3.812372320706767*^9, 
  3.812372354372938*^9}, {3.812374063796815*^9, 3.8123740639923964`*^9}},
