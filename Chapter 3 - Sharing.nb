9f39dbb9f36f"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"translateToText", "[", "money_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"money", ">", "0"}], ",", 
    RowBox[{"\"\< paid $\>\"", "<>", 
     RowBox[{"ToString", "[", "money", "]"}], "<>", "\"\< to the pot.\>\""}], 
    ",", 
    RowBox[{"\"\< received $\>\"", "<>", 
     RowBox[{"ToString", "[", 
      RowBox[{"-", "money"}], "]"}], "<>", "\"\< from the pot.\>\""}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "\[LessEqual]", "numPeople"}], ",", 
   RowBox[{"i", "++"}], ",", 
   RowBox[{"Print", "[", 
    RowBox[{
     RowBox[{"people", "[", 
      RowBox[{"[", "i", "]"}], "]"}], "<>", "\"\< received \>\"", "<>", 
     RowBox[{"itemsReceived", "[", 
      RowBox[{"[", "i", "]"}], "]"}], "<>", "\"\<and\>\"", "<>", 
     RowBox[{"translateToText", "[", 
      RowBox[{"paidToPot", "[", 
       RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "]"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.8123738659258738`*^9, 3.8123738785426273`*^9}, {
